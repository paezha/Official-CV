@article{ WOS:000285953000004,
Author = {Morency, Catherine and Paez, Antonio and Roorda, Matthew J. and Mercado,
   Ruben and Farber, Steven},
Title = {Distance traveled in three Canadian cities: Spatial analysis from the
   perspective of vulnerable population segments},
Journal = {JOURNAL OF TRANSPORT GEOGRAPHY},
