@article{ WOS:000354655700013,
Author = {Moniruzzaman, Md and Paez, Antonio and Scott, Darren and Morency,
   Catherine},
Title = {Trip generation of seniors and the geography of walking in Montreal},
Journal = {ENVIRONMENT AND PLANNING A-ECONOMY AND SPACE},
Year = {2015},
Volume = {47},
Number = {4},
Pages = {957-976},
Abstract = {With the graying of populations across the world, the travel behavior of
   seniors has become a topic of growing interest in planning and research.
   Most attention in the field of transportation has been devoted to
   motorized travel. However, the use of various modes of transportation,
   including nonmotorized travel, remains to be fully investigated. In this
   paper the multimodal trip generation of seniors in Montreal Island is
   studied. Personal, mobility tools, neighborhood, and accessibility
   variables are considered in a trivariate ordered probit model of three
   modes: car, transit, and walking. Geographical analysis of the walking
   component of the model helps to identify locations within the region
   where walking is more or less prevalent among older adults.},
