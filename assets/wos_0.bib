@article{ WOS:000179295600003,
Author = {Black, JA and Paez, A and Suthanaya, PA},
Title = {Sustainable urban transportation: Performance indicators and some
   analytical approaches},
Journal = {JOURNAL OF URBAN PLANNING AND DEVELOPMENT},
