@article{ WOS:000315474200013,
Author = {Paez, Antonio},
Title = {Mapping travelers' attitudes: does space matter?},
Journal = {JOURNAL OF TRANSPORT GEOGRAPHY},
Year = {2013},
Volume = {26},
Pages = {117-125},
Month = {JAN},
Abstract = {Exploratory Spatial Data Analysis of attitudinal variables can provide
   useful information about underlying behavioral processes. In this paper,
   elemental symbol mapping and spatial association analysis techniques are
   applied to travelers' attitudes. Attitudinal data, drawn from a recent
   travel survey conducted in McMaster University (Hamilton, Canada) among
   staff, students, and faculty, are analyzed. The place of residence of
   the respondents is geocoded in order to map the responses, and the
   spatial pattern of the qualitative/ordinal attitudinal variables is
   analyzed using the Q(m) statistic. Analysis of a selection of variables
   shows how some attitudes appear to develop independently from the local
   environments, whereas others are related to a preference towards certain
   types of local environments or social aspects of the local environment,
   which suggests underlying processes of self-selection or adaptation. (C)
   2012 Elsevier Ltd. All rights reserved.},
