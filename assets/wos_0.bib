@article{ WOS:000294484500002,
Author = {Schettini, Daniela and Azzoni, Carlos R. and Paez, Antonio},
Title = {Neighborhood and Efficiency in Manufacturing in Brazilian Regions: A
   Spatial Markov Chain Analysis},
Journal = {INTERNATIONAL REGIONAL SCIENCE REVIEW},
Year = {2011},
Volume = {34},
Number = {4},
Pages = {397-418},
Month = {OCT},
Abstract = {This paper analyzes the geography of regional competitiveness in
   manufacturing in Brazil. The authors estimate stochastic frontiers to
   calculate regional efficiency of representative firms in 137 regions in
   the period 2000-2006, in four sectors defined by technological
   intensity. The efficiency results are analyzed using Markov Spatial
   Transition Matrices to provide insights into the transition of regions
   between efficiency levels, considering their local spatial context. The
   results indicate that geography plays an important role in manufacturing
   competitiveness. In particular, regions with more competitive neighbors
   are more likely to improve their relative efficiency (pull effect) over
   time, and regions with less competitive neighbors are more likely to
   lose relative efficiency (drag effect). The authors find that the pull
   effect is stronger than the drag effect.},
