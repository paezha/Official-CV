@inproceedings{ WOS:000229696900018,
Author = {Páez, A},
Editor = {Gervasi, O and Gavrilova, ML and Kumar, V and Lagana, A and Lee, HP and Mun, Y and Taniar, D and Tan, CJK},
Title = {Local analysis of spatial relationships:: A comparison of GWR and the
   expansion method},
Booktitle = {COMPUTATIONAL SCIENCE AND ITS APPLICATIONS - ICCSA 2005, PT 3},
Series = {LECTURE NOTES IN COMPUTER SCIENCE},
Year = {2005},
Volume = {3482},
