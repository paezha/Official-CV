@article{ WOS:000659826500001,
Author = {Desjardins, Elise and Higgins, Christopher D. and Scott, Darren M. and
   Apatu, Emma and Paez, Antonio},
Title = {Correlates of bicycling trip flows in Hamilton, Ontario: fastest,
   quietest, or balanced routes?},
Journal = {TRANSPORTATION},
