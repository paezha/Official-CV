@article{ WOS:000910599000001,
Author = {Soukhov, Anastasia and Paez, Antonio},
Title = {TTS2016R: A data set to study population and employment patterns from
   the 2016 Transportation Tomorrow Survey in the Greater Golden Horseshoe
   area, Ontario, Canada},
